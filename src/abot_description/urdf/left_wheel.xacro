<?xml version="1.0"?>
<robot name="abot"
	xmlns:xacro="http://www.ros.org/wiki/xacro">
	<!-- Left wheel CAD link -->
	<link name="left_wheel1">
		<inertial>
			<origin xyz="7.0481E-20 -0.00013293 0" rpy="0 0 0" />
			<mass value="${wheel_mass}" />
			<inertia ixx="1.034E-05" ixy="6.5778E-20" ixz="-1.1387E-12" iyy="1.9808E-05" iyz="9.9943E-20" izz="1.034E-05" />
		</inertial>
		<visual>
			<origin xyz="0 0 0" rpy="0 0 0" />
			<geometry>
				<mesh filename="package://abot_description/meshes/left_wheel.STL" />
			</geometry>
			<material name="Green" />
		</visual>
		<collision>
			<origin xyz="0 0 0" rpy="0 0 0" />
			<geometry>
				<mesh filename="package://abot_description/meshes/left_wheel.STL" />
			</geometry>
		</collision>
	</link>
	<!-- Left wheel geometry link-->
	<link name="left_wheel">
		<inertial>
			<origin xyz="7.0481E-20 -0.00013293 0" rpy="0 0 0" />
			<mass value="${wheel_mass}" />
			<xacro:cylinder_inertia mass="${wheel_mass}" radius="${wheel_radius}" height="${wheel_width}" />
		</inertial>
		<visual>
			<origin xyz="0 0 0" rpy="${PI/2} 0 0" />
			<geometry>
				<cylinder length="${wheel_width}" radius="${wheel_radius}" />
			</geometry>
			<material name="Green" />
		</visual>
		<collision>
			<origin xyz="0 0 0" rpy="${PI/2} 0 0" />
			<geometry>
				<cylinder length="${wheel_width}" radius="${wheel_radius}" />
			</geometry>
		</collision>
	</link>
	<!-- Left wheel joints -->
	<joint name="left_wheel_to_base" type="continuous">
		<origin xyz="0 0.0639999999999998 0.0145000000000007" rpy="0 0 0" />
		<parent link="abot_base" />
		<child link="left_wheel" />
		<axis xyz="0 -1 0" />
	</joint>
	<joint name="left_wheel_to_wheel1" type="fixed">
		<parent link="left_wheel" />
		<child link="left_wheel1" />
		<origin xyz="0 0 0" rpy="0 0 0" />
	</joint>
</robot>
