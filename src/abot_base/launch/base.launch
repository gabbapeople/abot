
<launch>
      <!-- Load robot model -->
	<param name="robot_description" command="$(find xacro)/xacro '$(find abot_description)/urdf/abot.xacro' --inorder"/>
	
      <!-- Base node -->
      <node name="abot_base_node" pkg="abot_base" type="abot_base_node" output="screen">
            <param name="control_frequency" type="double" value="10.0"/>
	      <param name="max_wheel_angular_speed" type="double" value="20.8"/>
      </node>
	<!-- Load controller config -->
	<rosparam command="load" file="$(find abot_control)/config/abot_mobile.yaml"/>
	<rosparam command="load" file="$(find abot_control)/config/abot_control.yaml"/>
	<node name="controller_spawner" 
            pkg="controller_manager" 
            type="spawner" 
            output="screen" 
            args="/abot/mobile_abot abot/joint_state_controller"/>
	<!-- ! -->
	<node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" />
</launch>
